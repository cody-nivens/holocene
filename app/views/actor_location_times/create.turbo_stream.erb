<%= turbo_stream.append "#{dom_id @actor_location_time.actor}" do %>
  <% my_act_locs = ActorLocationTime.includes(:location_time).includes(:location).where("location_times.date_string": @actor_location_time.location_time.date_string, actor_id: @actor_location_time.actor.id).order("locations.name") %>
  <%= render partial: "actor_location_times/table_actors", locals: { my_act_locs:  my_act_locs %>
<% end %>
<%= render partial: "shared/send_flash" %>
