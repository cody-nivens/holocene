<%= turbo_stream.replace "sub_objects"  do %>
  <%= content_tag "turbo-frame", id: "sub_objects" do %>
    <%= render partial: 'shared/scenes', locals: { scenes: @scenes, op: 'scenes', option: @option, toggle: @toggle, long: @long } %>
  <% end %>
<% end %>
<%= turbo_stream.replace "new_object"  do %>
  <%= content_tag "turbo-frame", id: "new_object" do %>
    <%= render partial: "scenes/new_link" %>
  <% end %>
<% end %>
<%= render partial: "shared/send_flash" %>

