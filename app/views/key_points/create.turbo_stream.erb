<%= turbo_stream.replace "sub_objects"  do %>
  <%= content_tag :div, id: "sub_objects" do %>
    <%= render partial: "key_points/key_points", locals: { scripted: @scripted, long: @long, link: false } %>
  <% end %>
<% end %>
<%= turbo_stream.replace "edit"  do %>
  <%= content_tag :div, id: "edit" do %>
    <%= " " %>
  <% end %>
<% end %>
<%= turbo_stream.replace "new_object"  do %>
  <%= content_tag "turbo-frame", class: "block", id: "new_object" do %>
    <%= render partial: "key_points/new_link", locals: { scripted: @scripted} %>
  <% end %>
<% end %>
<%= render partial: "shared/send_flash" %>
