<%== link_to (fa_icon 'edit', class: 'fa-fw'), edit_key_point_path(@key_point), title: 'Edit', data: { "turbo-frame": "new_object" }, class: 'btn' %>
  <%== link_to (fa_icon 'eye', class: 'fa-fw'), key_point_view_path(@key_point), method: :get, title: 'View', class: 'btn' %>
  <%== link_to (fa_icon 'trash', class: 'fa-fw'), key_point_path(@key_point), method: :delete, data: { confirm: 'Are you sure?' }, title: 'Destroy', class: 'btn' %>
  <%== link_to (fa_icon 'truck', class: 'fa-fw'), polymorphic_path([@scripted, @key_point, :move]), method: :get, class: 'btn' %>
<% prev_key_point = @key_point.set_prev %>
<% unless prev_key_point.nil? %>
  <%= link_to (fa_icon 'angle-left', class: 'fa-fw'), key_point_path(prev_key_point), method: :get, title: 'Previous Key Point', class: 'btn' %>
<% end %>
<% next_key_point = @key_point.set_next %>
<% unless next_key_point.nil? %>
  <%= link_to (fa_icon 'angle-right', class: 'fa-fw'), key_point_path(next_key_point), method: :get, title: 'Next Key Point', class: 'btn' %>
<% end %>
<%= link_to (fa_icon 'backward', class: 'fa-fw'),  polymorphic_path(@scripted), method: :get, class: 'btn', title: 'Back' %>
