<%= render 'shared/map', object: @object %>

<% @footer_content << (link_to 'Timeline', url_for(controller: 'timelines', action: 'timeline', "#{@object.class.name.underscore}_id".to_sym => @object.id), class: 'btn btn-default').to_s %>
<% @footer_content << (link_to 'Display', "/#{@object.class.name.underscore.pluralize}/display/#{@object.id}", class: 'btn btn-default').to_s %>
<% @footer_content << (link_to 'Back', polymorphic_path(@object), class: 'btn btn-default').to_s %>
