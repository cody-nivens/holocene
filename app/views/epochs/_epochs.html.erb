<turbo-frame id="objects">
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Events</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @epochs.each do |epoch| %>
      <turbo-frame id="<%= dom_id epoch %>">
        <%= render partial: "epochs/epoch_item", locals: { epoch: epoch } %>
      </turbo-frame>
    <% end %>
  </tbody>
</table>
</turbo-frame>
