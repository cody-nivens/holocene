<div data-controller='grids' data-grids-gridtype-value="holocene_event" data-grids-url-value='<%= holocene_events_path %>'>
  <div data-grids-target="grid_form">
    <%= datagrid_form_for @grid, method: :get, url: holocene_events_path, partials: 'holocene_event_grid', data: { action: "grids#processSearch" } %>
    <div id="grid_data" data-grids-target="grid_data">
      <%= paginate(@grid.assets) %>
      <%= datagrid_table @grid, partials: 'holocene_event_grid' %>
      <%= paginate(@grid.assets) %>
    </div>
  </div>
</div>
