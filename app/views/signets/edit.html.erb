<h2>Editing Signet - <%= @sigged.class.name %>: <%= @sigged.name %></h2>

<turbo-frame id="new_object">
  <turbo-frame id="<%= dom_id @signet %>">
    <%= render 'form', signet: @signet, sigged: @sigged %>
    <%= link_to (fa_icon 'backward', class: 'fa-fw'),  polymorphic_path([@sigged, :signets]), method: :get, class: 'btn', title: 'Back' %>
  </turbo-frame>
</turbo-frame>
