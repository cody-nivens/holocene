<!doctype html>
<html>
  <head>
    <meta charset='utf-8' />
    <!-- CSS only -->
    <%= stylesheet_include_tag "http://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" %>
    <%= wicked_pdf_stylesheet_link_tag "pdf" -%>
    <%= wicked_pdf_javascript_include_tag "number_pages" %>

<!-- JS, Popper.js, and jQuery -->
<%= javascript_include_tag "http://code.jquery.com/jquery-3.5.1.slim.min.js" %>
<%= javascript_include_tag "http://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" %>
<%= javascript_include_tag "http://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" %>
    <title>Test Chapter PDF</title>
  </head>
  <body onload='number_pages'>
    <div id="header">
      <%#= wicked_pdf_image_tag 'mysite.jpg' %>
    </div>
    <div id="content">
    <%= render 'chapters/show', formats: [:html], epub: false, chapter: @chapter %>
    </div>
  </body>
</html>
